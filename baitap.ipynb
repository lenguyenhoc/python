   "source": [
    "so_nguyen = [1, 3, 4, 7, 2, 0, 10, 8, 6, 9]\n",
    "so_chan = 0\n",
    "so_le = 0\n",
    "for n in so_nguyen:\n",
    "    if n % 2 == 0:\n",
    "       so_chan = so_chan + n\n",
    "    else:\n",
    "       so_le = so_le + n\n",
    "print(\"Dãy số =\", so_nguyen)\n",
    "print(\"Tổng số chẵn =\", so_chan)\n",
    "print(\"Tổng số lẻ =\", so_le)"
   ]
